{
  "env": {
    "NODE_ENV": "production",
    "NPM_CONFIG_OPTIONAL": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NPM_CONFIG_OPTIONAL": "true"
    }
  },
  "installCommand": "npm install && npm install --include=optional sharp@0.33.5",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.1.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": []
}
